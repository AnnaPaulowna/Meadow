{
  "schemaVersion": 1,
  "id": "meadow",
  "version": "1.2.0",
  "name": "Meadow",
  "description": "A small mod Vanilla+ Mod that aims to improve the meadow biome and adds a new way to create food. It also brings back a few animals from Minecraft:Earth and gives them a reason to be there beside of looking pretty.",
  "authors": [
    "satisfyu",
    "Cristelknight999",
    "BMJO"
  ],
  "contact": {
    "homepage": "https://legacy.curseforge.com/minecraft/mc-mods/welcome-to-meadow",
    "sources": "https://github.com/satisfyu/Meadow/"
  },

  "license": "ARR",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.satisfyu.meadow.fabric.MeadowFabric"
    ],
    "client": [
      "net.satisfyu.meadow.fabric.client.MeadowFabricClient"
    ],
    "terrablender":  [
      "net.satisfyu.meadow.fabric.terrablender.TerrablenderFabric"
    ],

    "rei_client": [
      "net.satisfyu.meadow.fabric.rei.MeadowReiClientPlugin"
    ],
    "doapi": [
      "net.satisfyu.meadow.util.MeadowDoAPI"
    ]
  },
  "mixins": [
    "meadow.mixins.json"
  ],

  "depends": {
    "fabric": "*",
    "minecraft": ">=1.19.2",
    "architectury": ">=6.5.77",
    "doapi": ">1.0.3"
  }
}